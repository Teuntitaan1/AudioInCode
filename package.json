{
  "name": "AudioInCode",
  "displayName": "Audio in code",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onStartupFinished"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "AudioInCode.tellFileName",
        "title": "Tell file name"
      },
      {
        "command": "AudioInCode.grabSelectedText",
        "title": "Grab selected text"
      },
      {
        "command": "AudioInCode.playSelectedAudio",
        "title": "Play selected"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "AudioInCode.grabSelectedText"
        },
        {
          "command": "AudioInCode.playSelectedAudio"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  }
}
